<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.umer.androidhackathone"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.umer.wingoku.androidhackathone.SplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
        <activity android:name="com.umer.wingoku.androidhackathone.StartingPoint">
        </activity>
        
        
        <activity android:name="com.umer.wingoku.androidhackathone.Hospital" >
        </activity>
        
        
        <activity android:name="com.umer.wingoku.androidhackathone.RateDoctor" >
        </activity>

        <service android:name="com.umer.wingoku.androidhackathone.LocationMoniteringService" >
        </service>
    </application>

</manifest>